# procfs
genfscon proc /qcom_flash                           u:object_r:proc_qcom_flash:s0
genfscon proc /force_reset                          u:object_r:proc_force_reset:s0
genfscon proc /pmicWd                               u:object_r:proc_pmic_watchdog:s0
genfscon proc /s2_reset_config                      u:object_r:proc_s2_reset_config:s0
genfscon proc /simholder                            u:object_r:proc_simholder:s0
genfscon proc /touchpanel                           u:object_r:proc_touchpanel:s0
genfscon proc /rf_cable_config                      u:object_r:proc_rf_config:s0
genfscon proc /rf_factory_mode                      u:object_r:proc_rf_config:s0

